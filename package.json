{
	"name": "sveltekit-zero-api",
	"version": "0.11.3",
	"scripts": {
		"dev": "pnpm add -D ./sveltekit-zero-api.tgz && set NODE_ENV=zeroapi && vite dev",
		"zeroapi": "pnpm add -D ./sveltekit-zero-api.tgz",
		"i": "node install",
		"build": "node pack",
		"package": "svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"chokidar": "^3.5.3",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.17",
		"sveltekit-zero-api": "file:sveltekit-zero-api.tgz",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.3"
	},
	"type": "module",
	"module": "esnext"
}
