{
	"name": "sveltekit-zero-api",
	"description": "Provides type-safety between front- and backend of your SvelteKit project",
	"version": "0.12.1",
	"scripts": {
		"d": "set NODE_ENV=zeroapi && vite dev",
		"dev": "pnpm add -D ./sveltekit-zero-api.tgz && set NODE_ENV=zeroapi && vite dev",
		"zeroapi": "pnpm add -D ./sveltekit-zero-api.tgz",
		"i": "node install",
		"build": "node pack",
		"package": "svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.5",
		"@sveltejs/package": "^1.0.2",
		"chokidar": "^3.5.3",
		"svelte": "^3.55.1",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.23",
		"sveltekit-zero-api": "file:sveltekit-zero-api.tgz",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.5.5",
		"svelte": "^3.55.1"
	},
	"repository": "github:refzlund/sveltekit-zero-api",
	"homepage": "https://github.com/Refzlund/sveltekit-zero-api",
	"bugs": {
		"url": "https://github.com/Refzlund/sveltekit-zero-api/issues"
	},
	"type": "module",
	"module": "esnext",
	"keywords": [
		"sveltekit",
		"api",
		"endpoint",
		"typesafety",
		"typescript"
	]
}
